{
    "model_name": "decalign",
    "use_bert": true,
    "use_finetune": true,
    "transformers": "bert",
    "pretrained": "bert-base-uncased",
    "need_data_aligned": true,
    "dst_feature_dim_nheads": [40, 10],
    "nlevels": 5,
    "conv1d_kernel_size_l": 1,
    "conv1d_kernel_size_a": 1,
    "conv1d_kernel_size_v": 1,
    "num_prototypes": 8,
    "lambda_ot": 0.1,
    "ot_num_iters": 50,
    "attn_dropout": 0.1,
    "attn_dropout_a": 0.0,
    "attn_dropout_v": 0.0,
    "relu_dropout": 0.1,
    "embed_dropout": 0.25,
    "res_dropout": 0.1,
    "output_dropout": 0.0,
    "text_dropout": 0.0,
    "attn_mask": true,
    "alpha1": 0.1,
    "alpha2": 0.1,
    "batch_size": 24,
    "learning_rate": 0.001,
    "weight_decay": 0.0,
    "num_epochs": 100,
    "patience": 20,
    "clip": 0.8,
    "when": 20,
    "factor": 0.1
}